/Users/tranminh/Training/react-training/src/components/Layout/HeaderCartBtn.js",[],"/Users/tranminh/Training/react-training/src/components/Meals/AvailableMeals.js",["78"],"import classes from \"./AvailableMeals.module.css\";\nimport Card from \"../UI/Card\";\nimport MealItem from \"./MealItem/MealItem\";\nimport { useEffect, useState } from \"react\";\n\nconst DUMMY_MEALS = [\n  {\n    id: \"m1\",\n    name: \"Sushi\",\n    description: \"Finest fish and veggies\",\n    price: 22.99,\n  },\n  {\n    id: \"m2\",\n    name: \"Schnitzel\",\n    description: \"A german specialty!\",\n    price: 16.5,\n  },\n  {\n    id: \"m3\",\n    name: \"Barbecue Burger\",\n    description: \"American, raw, meaty\",\n    price: 12.99,\n  },\n  {\n    id: \"m4\",\n    name: \"Green Bowl\",\n    description: \"Healthy...and green...\",\n    price: 18.99,\n  },\n];\n\nconst AvailableMeals = () => {\n  const [meals, setMeals] = useState([]);\n  const [isLoading, setIsLoading] = useState(true);\n  const [httpError, setHttpError] = useState();\n\n  useEffect(() => {\n    const fetchMeals = async function () {\n      const res = await fetch(\n        \"https://react-meals-80231-default-rtdb.firebaseio.com/meals.json\"\n      );\n      if (!res.ok) throw new Error(\"Something went wrong\");\n      const data = await res.json();\n\n      const meals = [];\n      for (const key in data) {\n        meals.push({\n          id: key,\n          name: data[key].name,\n          description: data[key].description,\n          price: data[key].price,\n        });\n      }\n\n      setMeals(meals);\n      setIsLoading(false);\n    };\n\n    fetchMeals().catch((err) => {\n      setIsLoading(false);\n      setHttpError(err.message);\n    });\n  }, []);\n\n  if (isLoading) {\n    return (\n      <section className={classes.MealsLoading}>\n        <p>Loading...</p>\n      </section>\n    );\n  }\n\n  if (httpError) {\n    return (\n      <section className={classes.MealsError}>\n        <p>{httpError}</p>\n      </section>\n    );\n  }\n\n  const mealsList = meals.map((meal) => (\n    <MealItem\n      id={meal.id}\n      key={meal.id}\n      name={meal.name}\n      description={meal.description}\n      price={meal.price}\n    />\n  ));\n\n  return (\n    <section className={classes.meals}>\n      <Card>\n        <ul>{mealsList}</ul>\n      </Card>\n    </section>\n  );\n};\n\nexport default AvailableMeals;\n","/Users/tranminh/Training/react-training/src/components/Meals/MealsSummary.js",[],"/Users/tranminh/Training/react-training/src/components/UI/Modal.js",[],"/Users/tranminh/Training/react-training/src/components/Cart/CartIcon.js",[],"/Users/tranminh/Training/react-training/src/components/UI/Card.js",[],"/Users/tranminh/Training/react-training/src/components/Meals/MealItem/MealItem.js",[],"/Users/tranminh/Training/react-training/src/components/Meals/MealItem/MealItemForm.js",[],"/Users/tranminh/Training/react-training/src/components/UI/Input.js",[],"/Users/tranminh/Training/react-training/src/components/Cart/Checkout.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":6,"column":7,"nodeType":"85","messageId":"86","endLine":6,"endColumn":18},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'DUMMY_MEALS' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]